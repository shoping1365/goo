{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "node"
    ],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true,
    "skipLibCheck": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "@/*": [
        "./app/*"
      ],
      "~/server/*": [
        "./server/*"
      ],
      "#imports": [
        ".nuxt/types/nitro-imports.d.ts"
      ]
    }
  },
  "include": [
    ".nuxt/nuxt.d.ts",
    ".nuxt/imports.d.ts",
    ".nuxt/components.d.ts",
    ".nuxt/types/nitro-imports.d.ts",
    "app/**/*",
    "server/**/*",
    "nuxt.config.ts"
  ],
  "exclude": [
    "node_modules",
    ".nuxt",
    ".output"
  ]
}